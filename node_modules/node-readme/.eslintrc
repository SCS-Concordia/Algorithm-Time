{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
        "classes": true,
        "jsx": true,
        "modules": true
    },
    "plugins": [
    ],
    "rules": {
      "quotes": [2, "single"],
      "eol-last": [0],
      "no-mixed-requires": [0],
      "no-underscore-dangle": [0]
    }
}
